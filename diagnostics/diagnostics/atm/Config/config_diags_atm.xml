<?xml version="1.0"?>

<!-- =========================================================================== -->
<!-- Atmoshperic Model Working Group Diagnostics XML                             -->
<!-- =========================================================================== -->

<config_definition version="1.0">

  <groups>
    <!-- =========================================================================== -->
    <!-- atm_settings - general settings to be applied to all diagnostics routines   -->
    <!-- NOTE: case specific environment variables from env_*.xml will be            -->
    <!-- gathered at runtime by the atm_diags_generator.py python script.            -->
    <!-- They do not need to be specified in this XML file.                          -->
    <!-- The full $PATH variable will be set in the atm_diags_generator.py file      -->
    <!-- using all the path settings defined here.                                   -->
    <!-- =========================================================================== -->

    <group name="global">
      <order>0</order>
      <comment>Environmental variables that control which AMWG diagnostics to produce</comment>

      <entry id="ATMDIAG_DIAG_VERSION"
             type="integer"
             valid_values=""
             value="150626"
             group="global"
             desc="Diagnostic package version"
             ></entry>

      <entry id="ATMDIAG_MODEL_VS_OBS"
             type="logical"
             valid_values="True/False"
             value="True"
             group="global"
             desc="Controls whether to generate model vs. observations diagnostics"
             ></entry>

      <entry id="ATMDIAG_MODEL_VS_MODEL"
             type="logical"
             valid_values="True/False"
             value="False"
             group="global"
             desc="Controls whether to generate model vs. model diagnostics"
             ></entry>

      <entry id="ATMDIAG_CLEANUP_FILES"
             type="logical"
             valid_values="True/False"
             value="False"
             group="global"
             desc="Remove any existing diagnostic files."
             ></entry>

      <entry id="ATMDIAG_OUTPUT_ROOT_PATH"
             type="char"
             valid_values=""
             value="$DOUT_S_ROOT/atm/proc/diag"
             group="global"
             desc="Diagnostics output root path. Final path will be of the form $ATMDIAG_OUTPUT_ROOT_PATH/test_casename-obs.firstyr1_lastyr1 or $ATMDIAG_OUTPUT_ROOT_PATH/test_casename.firstyr1_lastyr1-cntl_casename.firstyr2_lastyr2"
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Set test datset                                                                                -->
    <!-- ============================================================================================== -->

    <group name="test_params">
      <order>1</order>
      <comment>Set test datset</comment>

      <entry id="ATMDIAG_test_casename"
             type="char"
             valid_values=""
             value="$CASE"
             group="test_params"
             desc="Identifying test casename"
             ></entry>

      <entry id="ATMDIAG_test_modelstream"
             type="char"
             valid_values="cam.h0,cam2.h0"
             value="cam.h0"
             group="test_params"
             desc="Identifying test model stream"
             ></entry>

      <entry id="ATMDIAG_TEST_TIMESERIES"
             type="logical"
             valid_values="True/False"
             value="False"
             group="test_params"
             desc="Use variable time series files to compute test climos instead of history time slice input files."
             ></entry>

      <entry id="ATMDIAG_test_path_history"
             type="char"
             valid_values=""
             value="$DOUT_S_ROOT"
             group="test_params"
             desc="Path to root of test case archive."
             ></entry>

      <entry id="ATMDIAG_test_path_climo"
             type="char"
             valid_values=""
             value="$DOUT_S_ROOT/atm/proc/climo/$ATMDIAG_test_casename/"
             group="test_params"
             desc="Path to generated test climos"
             ></entry>

      <entry id="ATMDIAG_test_path_diag"
             type="char"
             valid_values=""
             value="$DOUT_S_ROOT/atm/proc/diag/$ATMDIAG_test_casename/"
             group="test_params"
             desc="Path to generated diags."
             ></entry>

      <entry id="ATMDIAG_test_compute_climo"
             type="logical"
             valid_values="True/False"
             value="True"
             group="test_params"
             desc="Compute climatologies for the test case."
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Set cntl datset                                                                                -->
    <!-- ============================================================================================== -->
    <group name="cntl_params">
      <order>2</order>
      <comment>Set control datset</comment>

      <entry id="ATMDIAG_cntl_casename"
             type="char"
             valid_values=""
             value=""
             group="cntl_params"
             desc="Identifying cntl casename"
             ></entry>

      <entry id="ATMDIAG_cntl_modelstream"
             type="char"
             valid_values="cam.h0,cam2.h0"
             value="cam.h0"
             group="cntl_params"
             desc="Identifying control model stream"
             ></entry>

      <entry id="ATMDIAG_CNTL_TIMESERIES"
             type="logical"
             valid_values="True/False"
             value="False"
             group="cntl_params"
             desc="Use variable time series files to compute control climos instead of history time slice input files."
             ></entry>

      <entry id="ATMDIAG_cntl_path_history"
             type="char"
             valid_values=""
             value=""
             group="cntl_params"
             desc="Path to root of control case archive."
             ></entry>

      <entry id="ATMDIAG_cntl_path_climo"
             type="char"
             valid_values=""
             value="$DOUT_S_ROOT/atm/proc/climo/$ATMDIAG_cntl_casename/"
             group="cntl_params"
             desc="Path to generated control climos"
             ></entry>

      <entry id="ATMDIAG_cntl_compute_climo"
             type="logical"
             valid_values="True/False"
             value="False"
             group="cntl_params"
             desc="Compute climatologies for the control case."
             ></entry>
    </group>

    <!-- ============================================================================================== -->
    <!-- Set date ranges                                                                                -->
    <!-- ============================================================================================== -->

    <group name="date_params">
      <order>3</order>
      <comment>Set date ranges</comment>

      <entry id="ATMDIAG_test_first_yr"
             type="integer"
             valid_values=""
             value=""
             group="date_params"
             desc="Start year for test case for diagnostics."
             ></entry>

      <entry id="ATMDIAG_test_nyrs"
             type="integer"
             valid_values=""
             value=""
             group="date_params"
             desc="The number of years of test data to be used for the diagnostics."
             ></entry>

      <entry id="ATMDIAG_cntl_first_yr"
             type="integer"
             valid_values=""
             value=""
             group="date_params"
             desc="Start year for control case for diagnostics."
             ></entry>

      <entry id="ATMDIAG_cntl_nyrs"
             type="integer"
             valid_values=""
             value=""
             group="date_params"
             desc="The number of years of control data to be used for the diagnostics."
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Input data paths                                                                               -->
    <!-- ============================================================================================== -->

    <group name="input_data_paths">
      <order>4</order>
      <comment>Set input data paths</comment>

      <entry id="ATMDIAG_OBS_DATA"
             type="char"
             valid_values="$ATMDIAG_DIAGOBSROOT/amwg/obs_data"
             value=""
             group="input_data_paths"
             desc="Local machine fully qualified directory path to AMWG observational data."
             ></entry>

      <entry id="ATMDIAG_CAM35_DATA"
             type="char"
             valid_values="$ATMDIAG_DIAGOBSROOT/amwg/cam35_data"
             value=""
             group="input_data_paths"
             desc="Local machine fully qualified directory path to AMWG comparison data."
             ></entry>

      <entry id="ATMDIAG_MAP_DATA"
             type="char"
             valid_values="$ATMDIAG_DIAGOBSROOT/amwg/map_files"
             value=""
             group="input_data_paths"
             desc="Local machine fully qualified directory path to AMWG mapping data."
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Set climo settings                                                                             -->
    <!-- ============================================================================================== -->

    <group name="climo_settings">
      <order>5</order>
      <comment>Set climatology specific settings</comment>

      <entry id="ATMDIAG_strip_off_vars"
             type="logical"
             valid_values="True/False"
             value="True"
             group="climo_settings"
             desc="Strip off unused variables in the climo files."
             ></entry>

      <entry id="ATMDIAG_weight_months"
             type="logical"
             valid_values="True/False"
             value="True"
             group="climo_settings"
             desc="Weight the months by their number of days."
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Select Plot Sets										    --> 
    <!-- ============================================================================================== -->

    <group name="plot_sets">
      <order>6</order>
      <comment>Select plot sets</comment>

      <entry id="ATMDIAG_all_sets"
             type="logical"
             valid_values="True/False"
             value="True"
             group="plot_sets"
             desc="Toggle to produce all standard plotting sets."
             ></entry>

      <entry id="ATMDIAG_set_1"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 1 - tables of global,regional means"
             ></entry>

      <entry id="ATMDIAG_set_2"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 2 - implied transport plots"
             ></entry>

      <entry id="ATMDIAG_set_3"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 3 - zonal mean line plots"
             ></entry>

      <entry id="ATMDIAG_set_4"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 4 - vertical zonal mean contour plots"
             ></entry>
      <entry id="ATMDIAG_set_4a"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 4a - vertical zonal mean contour plots"
             ></entry>

      <entry id="ATMDIAG_set_5"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 5 - 2D-field contour plots"
             ></entry>

      <entry id="ATMDIAG_set_6"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 6 - 2D-field vector plots"
             ></entry>

      <entry id="ATMDIAG_set_7"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 7 - 2D-field polar plots"
             ></entry>

      <entry id="ATMDIAG_set_8"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 8 - annual cycle (vs lat) contour plots"
             ></entry>

      <entry id="ATMDIAG_set_9"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 9 - DJF-JJA difference plots"
             ></entry>

      <entry id="ATMDIAG_set_10"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 10 - annual cycle line plots"
             ></entry>

      <entry id="ATMDIAG_set_11"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 11 - miscellaneous plots"
             ></entry>

      <entry id="ATMDIAG_set_12"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 12 - stations"
             ></entry>

      <entry id="ATMDIAG_set_13"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 13 - COSP cloud simulator plots"
             ></entry>

      <entry id="ATMDIAG_set_14"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 14 - Taylor diagram plots"
             ></entry>

      <entry id="ATMDIAG_set_15"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 15 - Annual Cycle Plots for Select stations"
             ></entry>

      <entry id="ATMDIAG_set_16"
             type="logical"
             valid_values="True/False"
             value="False"
             group="plot_sets"
             desc="Run plotting set 16 - Budget Terms for Select stations"
             ></entry>

      <entry id="ATMDIAG_TAYLOR_BASECASE"
             type="char"
             valid_values="ccsm3_5/cam3_5"
             value="ccsm3_5"
             group="diags_atm"
             desc="Cntl case to compare against for Taylor Diagrams"
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Select WACCM Plot Sets										    --> 
    <!-- ============================================================================================== -->

    <group name="waccm_plot_sets">
      <order>7</order>
      <comment>Select WACCM plot sets</comment>

      <entry id="ATMDIAG_all_waccm_sets"
             type="logical"
             valid_values="True/False"
             value="False"
             group="waccm_plot_sets"
             desc="Run all the WACCM sets"
             ></entry>

      <entry id="ATMDIAG_wset_1"
             type="locical"
             valid_values="True/False"
             value="False"
             group="waccm_plot_sets"
             desc="Run WACCM set 1 - vertical zonal mean contour plots (log scale)"
             ></entry>
    </group>


    <!-- ============================================================================================== -->
    <!-- Select Chemistry Plot Sets										    --> 
    <!-- ============================================================================================== -->

    <group name="chem_plot_sets">
      <order>8</order>
      <comment>Select chemistry plot sets</comment>

      <entry id="ATMDIAG_all_chem_sets"
             type="logical"
             valid_values="True/False"
             value="False"
             group="chem_plot_sets"
             desc="Run all the CHEM sets"
             ></entry>

      <entry id="ATMDIAG_cset_1"
             type="logical"
             valid_values="True/False"
             value="False"
             group="chem_plot_sets"
             desc="Run CHEM set 1 - tables of global budgets"
             ></entry>

      <entry id="ATMDIAG_cset_2"
             type="logical"
             valid_values="True/False"
             value="False"
             group="chem_plot_sets"
             desc="Run CHEM set 2 - vertical zonal mean contour plots (log scale)"
             ></entry>

      <entry id="ATMDIAG_cset_3"
             type="logical"
             valid_values="True/False"
             value="False"
             group="chem_plot_sets"
             desc="Run CHEM set 3 - Ozonesonde comparisions"
             ></entry>

      <entry id="ATMDIAG_cset_4"
             type="logical"
             valid_values="True/False"
             value="False"
             group="chem_plot_sets"
             desc="Run CHEM set 4 - Column Ozone/CO Comparisons"
             ></entry>

      <entry id="ATMDIAG_cset_5"
             type="logical"
             valid_values="True/False"
             value="False"
             group="chem_plot_sets"
             desc="Run CHEM set 5 - NOAA Aircraft comparisons"
             ></entry>

      <entry id="ATMDIAG_cset_6"
             type="logical"
             valid_values="True/False"
             value="False"
             group="chem_plot_sets"
             desc="Run CHEM set 6 - Emmons Aircraft climatology"
             ></entry>

      <entry id="ATMDIAG_cset_7"
             type="logical"
             valid_values="True/False"
             value="False"
             group="chem_plot_sets"
             desc="Run CHEM set 7 - surface comparisons (ozone, co, improve)"
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Seasonal plot selection                                                                        -->
    <!-- ============================================================================================== -->

    <group name="seasons">
      <order>9</order>
      <comment>Select which seasons to generate climatolgies and plots</comment>

      <entry id="ATMDIAG_plot_ANN_climo"
             type="logical"
             valid_values="True/False"
             value="True"
             group="seasons"
             desc="Create Annual plots - used by sets 1-7,11"
             ></entry>

      <entry id="ATMDIAG_plot_DJF_climo"
             type="logical"
             valid_values="True/False"
             value="True"
             group="seasons"
             desc="Create winter plots - used by sets 1,3-7,9,11"
             ></entry>

      <entry id="ATMDIAG_plot_MAM_climo"
             type="logical"
             valid_values="True/False"
             value="True"
             group="seasons"
             desc="Create Spring plots - used by sets 1,3-7,9,11"
             ></entry>

      <entry id="ATMDIAG_plot_JJA_climo"
             type="logical"
             valid_values="True/False"
             value="True"
             group="seasons"
             desc="Create Summer plots - used by sets 1,3-7,9,11"
             ></entry>

      <entry id="ATMDIAG_plot_SON_climo"
             type="logical"
             valid_values="True/False"
             value="True"
             group="seasons"
             desc="Create Autum plots - used by sets 1,3-7,9,11"
             ></entry>

      <entry id="ATMDIAG_plot_MON_climo"
             type="logical"
             valid_values="True/False"
             value="True"
             group="seasons"
             desc="Create monthly plots - used by sets 8,10,11,12"
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Customize plots                                                                                -->
    <!-- ============================================================================================== -->

    <group name="custom_plots">
      <order>10</order>
      <comment>Customized plot settings</comment>

      <entry id="ATMDIAG_p_type"
             type="char"
             valid_values="ps/png"
             value="png"
             group="custom_plots"
             desc="File type for plots"
             ></entry>

      <entry id="ATMDIAG_c_type"
             type="char"
             valid_values="COLOR/MONO"
             value="COLOR"
             group="custom_plots"
             desc="Output color type"
             ></entry>

      <entry id="ATMDIAG_color_bar"
             type="char"
             valid_values="default/blue_red/blue_yellow_red"
             value="blue_red"
             group="custom_plots"
             desc="Plot color scheme"
             ></entry>

      <entry id="ATMDIAG_time_stamp"
             type="logical"
             valid_values="True/False"
             value="False"
             group="custom_plots"
             desc="Add timestamp to plots"
             ></entry>

      <entry id="ATMDIAG_tick_marks"
             type="logical"
             valid_values="True/False"
             value="False"
             group="custom_plots"
             desc="Add tick marks and labels to plot sets 5,6,7"
             ></entry>

      <entry id="ATMDIAG_custom_names"
             type="logical"
             valid_values="True/False"
             value="False"
             group="custom_plots"
             desc="Use custom names for plots"
             ></entry>

      <entry id="ATMDIAG_test_name"
             type="char"
             valid_values=""
             value="$ATMDIAG_test_casename"
             group="custom_plots"
             desc="Custom test name for plots"
             ></entry>

      <entry id="ATMDIAG_cntl_name"
             type="char"
             valid_values=""
             value="$ATMDIAG_cntl_casename"
             group="custom_plots"
             desc="Custom cntl name for plots"
             ></entry>

      <entry id="ATMDIAG_web_pages"
             type="logical"
             valid_values="True/False"
             value="True"
             group="custom_plots"
             desc="Create webpages"
             ></entry>

      <entry id="ATMDIAG_save_ncdfs"
             type="logical"
             valid_values="True/False"
             value="False"
             group="custom_plots"
             desc="Save temp netCDF files created by plotting scripts."
             ></entry>

      <entry id="ATMDIAG_significance"
             type="logical"
             valid_values="True/False"
             value="False"
             group="custom_plots"
             desc="Compute significance"
             ></entry>

      <entry id="ATMDIAG_sig_lvl"
             type="real"
             valid_values=""
             value="0.05"
             group="custom_plots"
             desc="Level of significance"
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Additional settings                                                                            -->
    <!-- ============================================================================================== -->

    <group name="additional_settings">
      <order>11</order>
      <comment>Additional misc. settings</comment>

      <entry id="ATMDIAG_DIAG_HOME"
             type="char"
             valid_values=""
             value="$POSTPROCESS_PATH/atm_diag"
             group="additional_settings"
             desc="Diagnostic package location"
             ></entry>

      <entry id="ATMDIAG_NCLPATH"
             type="char"
             valid_values=""
             value="$ATMDIAG_DIAG_HOME"
             group="additional_settings"
             desc="Local machine path to AMWG diagnostics package NCL library directory."
             ></entry>

      <entry id="ATMDIAG_WEBDIR_MODEL_VS_OBS"
             type="char"
             valid_values=""
             value=""
             group="web_paths"
             desc="Web directory path settings set automatically by atm_diags_generator.py for MODEL_VS_OBS diagnostics"
             ></entry>

      <entry id="ATMDIAG_WEBDIR_MODEL_VS_MODEL"
             type="char"
             valid_values=""
             value=""
             group="web_paths"
             desc="Web directory path settings set automatically by atm_diags_generator.py for MODEL_VS_MODEL diagnostics"
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Interpolation settings                                                                         -->
    <!-- ============================================================================================== -->

    <group name="interp_settings">
      <order>12</order>
      <comment>Interpolation settings</comment>

      <entry id="ATMDIAG_INTERP_METHOD"
             type="char"
             valid_values="bilinear/patch/conserve"
             value="bilinear"
             group="interp_settings"
             desc="Interpolation method"
             ></entry>

      <entry id="ATMDIAG_test_regrid"
             type="char"
             valid_values="True/False"
             value="False"
             group="interp_settings"
             desc="Interpolate the test climo files"
             ></entry>

      <entry id="ATMDIAG_test_res_in"
             type="char"
             valid_values=""
             value="$ATM_GRID"
             group="interp_settings"
             desc="Original test climo resolution"
             ></entry>

      <entry id="ATMDIAG_test_res_out"
             type="char"
             valid_values="0.9x1.25/"
             value="0.9x1.25"
             group="interp_settings"
             desc="Interpolated test climo FV resolution"
             ></entry>

      <entry id="ATMDIAG_cntl_regrid"
             type="char"
             valid_values="True/False"
             value="False"
             group="interp_settings"
             desc="Interpolate the cntl climo files"
             ></entry>

      <entry id="ATMDIAG_cntl_res_in"
             type="char"
             valid_values=""
             value="$ATM_GRID"
             group="interp_settings"
             desc="Original cntl climo resolution"
             ></entry>

      <entry id="ATMDIAG_cntl_res_out"
             type="char"
             valid_values="0.9x1.25/"
             value="0.9x1.25"
             group="interp_settings"
             desc="Interpolated cntl climo FV resolution"
             ></entry>

    </group>

    <!-- ============================================================================================== -->
    <!-- Stations to plot (set 12)                                                                      -->
    <!-- ============================================================================================== -->

    <group name="stations">
      <order>13</order>
      <comment>Stations to plot (set 12)</comment>

      <!-- =============================== -->
      <!-- ARCTIC (60N-90N)                -->
      <!-- =============================== -->
      <entry id="ATMDIAG_western_alaska"
             type="logical"
             valid_values="True/False"
             value="False"
             group="stations"
             desc="Plot station western_alaska #24"
             ></entry>

      <entry id="ATMDIAG_whitehorse_canada"
             type="logical"
             valid_values="True/False"
             value="False"
             group="stations"
             desc="Plot station whitehorse_canada #45"
             ></entry>

      <entry id="ATMDIAG_resolute_canada"
             type="logical"
             valid_values="True/False"
             value="True"
             group="stations"
             desc="Plot station resolute_canada #5"
             ></entry>

      <entry id="ATMDIAG_thule_greenland"
             type="logical"
             valid_values="True/False"
             value="True"
             group="stations"
             desc="Plot station thule_greenland #25"
             ></entry>

      <!-- =============================== -->
      <!-- NORTHERN MIDLATITUDES (23N-60N) -->
      <!-- =============================== -->

      <entry id="ATMDIAG_new_dehli_india"
             type="logical"
             valid_values="True/False"
             value="False"
             group="stations"
             desc="Plot station new_dehli_india #51"
             ></entry>

      <entry id="ATMDIAG_kagoshima_japan"
             type="logical"
             valid_values="True/False"
             value="False"
             group="stations"
             desc="Plot station kagoshima_japan #21"
             ></entry>

      <entry id="ATMDIAG_tokyo_japan"
             type="logical"
