<h3>{{ title }}</h3>

<table class="plot">
{% for t in plot_table %}
    <tr>
    {% for i in t %}
        {% if 'Error' in i %}
           <td>{{ i|replace('- Error', ' ') }}</td>
        {% else %}
           <td><a href="{{ i }}.{{ imgFormat }}">{{ i }}</a></td>
        {% endif %}
    {% endfor %}
    </tr>
{% endfor %}
</table>

<hr>
