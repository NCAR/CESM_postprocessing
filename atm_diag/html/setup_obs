 echo $1 "<br>and<br>" $2 >> set16/set16_{$col}.htm
 cat set16/set16_3.htm | sed -e s/xxx/$3/  | sed -e s/lll/$col/ >> set16/set16_{$col}.htm
end

# WSET1
if (-e wset1/wset1.htm) then
  rm wset1/wset1.htm
